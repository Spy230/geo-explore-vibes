{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/www.yandex-maps-2gis-prodvigenie-phenix.ru/(.*)",
      "destination": "https://yandex-maps-2gis-prodvigenie-phenix.ru/$1",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "regions": ["iad1", "fra1", "hnd1"],
  "cleanUrls": true,
  "trailingSlash": false
}
